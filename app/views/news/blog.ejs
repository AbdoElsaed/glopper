<%- contentFor('title')%>
<%= title %>

<%- contentFor('breadcrumb_title')%>
<%= title %>

<%- contentFor('breadcrumb_item') %>
<li class="breadcrumb-item">Blog</li>
<li class="breadcrumb-item active"><%= title %></li>

<%- contentFor('body') %>
